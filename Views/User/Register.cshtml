<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, width=device-width" />

    <link rel="stylesheet" href="./global.css" />
    <link rel="stylesheet" href="./register.css" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Poppins:wght@500;600;700&display=swap"
    />

    <style>
    
    </style>
  </head>
  <body>
    <div class="register">
      <header class="logo-bg-void-1-parent">
        <img
          class="logo-bg-void-11"
          loading="lazy"
          alt=""
          src="./public/logo-bg-void-11@2x.png"
          id="logoBgVoid1"
        />

        <div class="frame-wrapper">
          <div class="home-parent">
            <div class="home1" id="homeText">Home</div>
            <div class="frame-div">
              <div class="administrator-wrapper">
                <div class="administrator" id="administratorText">
                  Administrator
                </div>
              </div>
              <div class="frame-parent1">
                <div class="all-parent">
                  <div class="all"></div>
                  <div class="register1">Register</div>
                </div>
                <div class="log-in" id="logInText">Log In</div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <main class="registration-content-wrapper">
        <section class="registration-content">
          <div class="registration-title-wrapper">
            <div class="registration-title">
              <h1 class="hello-newbie">Hello, Newbie!</h1>
              <div class="join-us-and">
                Join us and start reserving seats today!
              </div>
            </div>
          </div>
          <div class="frame-parent2">
            <div class="ellipse-parent">
              <div class="ellipse-div"></div>
              <div class="status-label"></div>
              <div class="frame-child3"></div>
              <div class="status"></div>
              <div class="frame-child4"></div>
            </div>
            <form class="divider-wrapper">
              <div class="divider-wrapper-child"></div>
              <div class="register-now-button-wrapper">
                <div class="register-now-button">
                  <h1 class="register-now">Register Now!</h1>
                  <div class="register-now-button-inner">
                    <div class="frame-parent3">
                      <input
                        class="group-input"
                        placeholder="Enter Username"
                        type="text"
                      />

                      <input
                        class="frame-child5"
                        placeholder="Enter Password"
                        type="text"
                      />

                      <input
                        class="frame-child5"
                        placeholder="Confirm Password"
                        type="text"
                      />

                      <input
                        class="frame-child5"
                        placeholder="Enter Email Address"
                        type="text"
                      />
                    </div>
                  </div>
                </div>
              </div>
              <div class="login-button-parent">
                <div class="register-now-button-wrapper">
                  <button class="group-button">
                    <div class="frame-child8"></div>
                    <div class="register2">Register</div>
                  </button>
                </div>
                <div class="if-you-already-container" id="ifYouAlready">
                  <span class="if-you-already"
                    >If you already have an Account?
                  </span>
                  <span class="login">Login</span>
                </div>
              </div>
            </form>
          </div>
        </section>
      </main>
    </div>

    <script>
      var logoBgVoid1 = document.getElementById("logoBgVoid1");
      if (logoBgVoid1) {
        logoBgVoid1.addEventListener("click", function (e) {
          window.location.href = "./home.html";
        });
      }
      
      var homeText = document.getElementById("homeText");
      if (homeText) {
        homeText.addEventListener("click", function (e) {
          window.location.href = "./home.html";
        });
      }
      
      var administratorText = document.getElementById("administratorText");
      if (administratorText) {
        administratorText.addEventListener("click", function (e) {
          window.location.href = "./admin-login.html";
        });
      }
      
      var logInText = document.getElementById("logInText");
      if (logInText) {
        logInText.addEventListener("click", function (e) {
          window.location.href = "./user-login.html";
        });
      }
      
      var ifYouAlready = document.getElementById("ifYouAlready");
      if (ifYouAlready) {
        ifYouAlready.addEventListener("click", function (e) {
          window.location.href = "./user-login.html";
        });
      }
      </script>
  </body>
</html>
